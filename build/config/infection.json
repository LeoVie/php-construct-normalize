{
  "timeout": 10,
  "source": {
    "directories": [
      "src"
    ]
  },
  "phpUnit": {
    "configDir": "."
  },
  "logs": {
    "text": "build/logs/infection.log"
  },
  "mutators": {
    "@default": true,
    "IncrementInteger": {
      "ignore": [
        "LeoVie\\PhpMethodModifier\\Service\\MethodModifierService::extractAccessModifier",
        "LeoVie\\PhpConstructNormalize\\Helper\\RandomNameGenerator::pickRandomCharacter"
      ]
    },
    "DecrementInteger": {
      "ignore": [
        "LeoVie\\PhpConstructNormalize\\Helper\\RandomNameGenerator::pickRandomCharacter"
      ]
    }
  },
  "tmpDir": "../cache"
}
